<table id='async_dataIntequipment' class="table-striped">
  <thead>
    <tr>
      <th>Created</th>
      <th>Equipment Name</th>
      <th>Ticket Number</th>
      <th>Note HS</th>
      <th>User</th>
      <th>Panel</th>
    </tr>
  </thead>

  <tbody>
      <% @intequipments.each do |intequipment| %>
        <tr>
          <td><%= intequipment.created_at %></td>
          <td><%= intequipment.equipmentName %></td>
          <td><%= intequipment.ticketN %></td>
          <td><%= intequipment.remHS %></td>
          <td><%= intequipment.user.name %></td>
          <td>
              <%= link_to 'Show', intequipment %>|
              <%= link_to 'Edit', edit_intequipment_path(intequipment) %>|
              <%= link_to 'Destroy', intequipment, method: :delete, data: { confirm: 'Are you sure?' } %>
          </td>
        </tr>
      <% end %>
  </tbody>
</table>
