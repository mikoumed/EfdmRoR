<table id='async_dataEvent' class="table-striped">
  <thead>
    <tr>
      <th>Opened</th>
      <th>Object</th>
      <th>Added by</th>
      <th>Content</th>
	  <th>Panel</th>
    </tr>
  </thead>

  <tbody>
      <% @events.each do |event| %>
        <tr>
          <td><%= event.created_at %></td>
          <td><%= event.object %></td>
          <td><%= event.user.name %></td>
          <td><%= event.content %></td>
          <td>
              <%= link_to 'Show', event %>|
              <%= link_to 'Edit', edit_event_path(event) %>|
              <%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %>
          </td>
        </tr>
      <% end %>
  </tbody>

</table>
